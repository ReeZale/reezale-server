    "chardet": "^2.1.0",
    "cheerio": "^1.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "fast-xml-parser": "^4.5.1",
    "file-type": "^16.5.4",
    "fs": "^0.0.1-security",
    "fuse.js": "^7.1.0",
    "he": "^1.2.0",
    "iconv-lite": "^0.6.3",
