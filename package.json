{
  "name": "reezale-scraper",
  "version": "1.0.0",
  "description": "custome data scraper",
  "main": "index.js",
  "scripts": {
    "build": "npx prisma generate && npx prisma generate --schema=prisma-directory/schema.prisma",
    "start": "node index.js",
    "start:prod": "npm run build && npm run migrate:deploy && node index.js",
    "migrate:deploy": "npx prisma migrate deploy --schema=prisma/schema.prisma && npx prisma migrate deploy --schema=prisma-directory/schema.prisma",
    "seed": "npx prisma db seed"
  },
  "prisma": {
    "seed": "node prisma/seed/index.js"
  },
  "author": "Zachary Rodney",
  "license": "ISC",
